{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
        "start": {
            "dependsOn": [
                "^start"
            ],
            "cache": false
        },
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**",
                "build/**"
            ]
        }
    }
}